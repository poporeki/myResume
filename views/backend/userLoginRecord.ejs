<%- include('common') -%>
<div class="pd-x-20 pd-sm-x-30 pd-t-20 pd-sm-t-30">
  <h4 class="tx-gray-800 mg-b-5 mg-t-10 mg-sm-t-0">账号登录记录</h4>
  <p class="mg-b-0">所有记录列表.</p>
</div>
<div class="br-pagebody">
  <div class="br-section-wrapper">
    <div class="row">
    <div class="col-md-12">
    <table class="table table-hover mg-b-0">
      <thead>
        <tr>
          <th>ID</th>
          <th>用户名</th>
          <th>登录设备</th>
          <th>登录浏览器</th>
          <th>位置信息</th>
          <th>ISP</th>
          <th>登录时间</th>
        </tr>
      </thead>
      <tbody>
      <%for(var i=0;i<record.length;i++){%>
      <%var data=record[i];%>
        <tr>
          <th scope="row"><%=i+1%></th>
          <td><%=data.username%> </td>
          <td><%=data.os%>  </td>
          <td><%=data.browser%></td>
          <td><%=data.ip%><br>
          <%=data.address%>
          </td>
          <td><%=data.isp%></td>
          <td><%=data.logtime%>  </td>
        </tr>
      <%}%>
      </tbody>
    </table>
    </div>
  </div>
  </div>
</div>


<%- include('footer') -%>